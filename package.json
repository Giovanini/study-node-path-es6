    "name": "Douglas Duteil",
    "email": "douglasduteil@gmail.com"
  },
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/douglasduteil/study-node-path-es6/issues"
  },
  "homepage": "https://github.com/douglasduteil/study-node-path-es6",
  "devDependencies": {
    "babel": "^4.0.1",
    "chai": "^1.10.0",
    "isparta": "^2.0.0",
    "mocha": "^2.1.0"
  }
}
