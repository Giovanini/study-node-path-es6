{
  "name": "study-node-path-es6",
  "version": "0.1.0",
  "description": "Studying how one would code the path module from node core in ES6 (inspired by substack/path-browserify) ",
  "main": "index.js",
  "scripts": {
    "test": "babel-node node_modules/.bin/_mocha $npm_package_config_MOCHA_OPTS",
    "coverage": "babel-node node_modules/.bin/isparta cover $npm_package_config_ISPARTA_OPTS node_modules/.bin/_mocha -- $npm_package_config_MOCHA_OPTS"
  },
  "config": {
    "MOCHA_OPTS": "--reporter dot",
    "ISPARTA_OPTS": "--report html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/douglasduteil/study-node-path-es6"
  },
  "keywords": [
    "path",
    "es6",
    "study"
  ],
